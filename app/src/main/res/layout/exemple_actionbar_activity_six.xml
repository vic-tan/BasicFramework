<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:abv="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">


    <com.tanlifei.support.widget.actionbar.ActionBarView
        android:id="@+id/actionbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_actionbar_standard_height"
        abv:abv_title_text="所有操作"
        abv:abv_opt_left_text="清空"
        abv:abv_back_text="返回"
        abv:abv_opt_left_iconDrawable_normol="@mipmap/exemple_icon_play_nor"
        abv:abv_opt_left_iconDrawable_pressed="@mipmap/exemple_icon_play_pre"
        abv:abv_opt_right_iconDrawable_normol="@mipmap/exemple_icon_play_nor"
        abv:abv_opt_right_iconDrawable_pressed="@mipmap/exemple_icon_play_pre"
        abv:abv_opt_right_text="添加"/>

</com.zhy.autolayout.AutoRelativeLayout>

